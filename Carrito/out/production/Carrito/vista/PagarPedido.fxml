<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="720.0" prefWidth="480.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.PagarPedidoController">

   <stylesheets>
      <URL value="@styles.css" />  </stylesheets>

    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <AnchorPane fx:id="panControll" prefHeight="885.0" prefWidth="676.0">
         <children>
            <TextField fx:id="txtTotal" layoutX="129.0" layoutY="222.0" prefHeight="52.0" prefWidth="182.0">
               <cursor>
                  <Cursor fx:constant="DEFAULT" />
               </cursor></TextField>
            <Label layoutX="197.0" layoutY="182.0" text="Total" />
            <Button fx:id="btnPagar" layoutX="179.0" layoutY="314.0" mnemonicParsing="false" onAction="#btnPagar" text="Pagar" />
         </children>
      </AnchorPane>
   </children>
</VBox>
